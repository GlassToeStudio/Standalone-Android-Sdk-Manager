<Application x:Class="GTS_SDK_Manager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GTS_SDK_Manager"
             StartupUri="Windows/MainWindow.xaml">
	<Application.Resources>
		
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<!-- Order is important -->
				<ResourceDictionary Source="Styles/Texts.xaml" />
				<ResourceDictionary Source="Styles/Buttons.xaml" />
				<ResourceDictionary Source="Styles/Scrolls.xaml" />
				<ResourceDictionary Source="Styles/CheckBoxes.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
			
	</Application.Resources>
</Application>
