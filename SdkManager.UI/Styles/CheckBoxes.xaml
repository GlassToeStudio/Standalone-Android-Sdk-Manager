<ResourceDictionary 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:local="clr-namespace:SdkManager.UI" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	
	<!--Foreground="#BBBBBB" BorderBrush="#FF2D2F31" BorderThickness="1"  Background="#686868"-->
	<Style TargetType="CheckBox">
		<Setter Property="Background" Value="#686868"></Setter>
		<Setter Property="BorderThickness" Value="1"></Setter>
		<Setter Property="BorderBrush" Value="#FF2D2F31"></Setter>
		<Setter Property="Foreground" Value="#BBBBBB"/>
	</Style>
	

</ResourceDictionary>